{
  "name": "arc-kiosk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "turbo-build": "turbo run build",
    "turbo-dev": "turbo run dev",
    "dev": "concurrently --kill-others-on-fail \"yarn run dev-frontend\" \"yarn run dev-backend\"",
    "dev-frontend": "vite",
    "dev-backend": ".venv/bin/python server/src/app.py",
    "build": "vite build",
    "check-types": "tsc -b --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "firebase:deploy:hosting": "turbo run build && firebase use default && firebase deploy --only hosting",
    "firebase:deploy:database": "firebase deploy --only database",
    "gloud:deploy": "cd server && gcloud builds submit --tag gcr.io/arc-kiosk/backend && gcloud run deploy --image gcr.io/arc-kiosk/backend --platform managed"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "axios": "^1.9.0",
    "firebase": "^11.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.5.2",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "turbo": "^2.5.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.2"
  },
  "packageManager": "yarn@4.6.0"
}
