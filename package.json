{
  "name": "arc-kiosk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "turbo-build": "turbo run build",
    "turbo-dev-linux": "turbo run dev-linux",
    "dev-linux": "concurrently --kill-others-on-fail \"yarn run dev-frontend\" \"yarn run dev-backend-linux\"",
    "dev-frontend": "vite",
    "dev-backend-linux": ".venv/bin/python server/src/app.py",
    "dev-backend-windows": "./.venv/Scripts/python.exe server/src/app.py",
    "dev-windows": "concurrently --kill-others-on-fail \"yarn run dev-frontend\" \"yarn run dev-backend-windows\"",
    "build": "vite build",
    "check-types": "tsc -b --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "firebase:deploy:hosting": "turbo run build && firebase use default && firebase deploy --only hosting",
    "firebase:deploy:database": "firebase deploy --only database",
    "gloud:deploy": "cd server && gcloud builds submit --tag gcr.io/arc-kiosk/backend && gcloud run deploy backend --image gcr.io/arc-kiosk/backend --platform managed"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.2",
    "firebase": "^12.6.0",
    "framer-motion": "^12.23.26",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.10.1",
    "react-webcam": "^7.2.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.25",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "turbo": "^2.6.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0"
  },
  "packageManager": "yarn@4.12.0"
}
